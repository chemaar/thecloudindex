	rdfs:label "Compliance"@en ;
	rdfs:comment "Standards, processes, and policies committed to by the service provider are followed."@en ;
	skos:definition "Standards, processes, and policies committed to by the service provider are followed."@en ;
	ci-onto:defined-in <http://csmic.org/wp-content/uploads/2011/09/SMI-Overview-110913_v1F.pdf>;
