ci-indicator:DataOwnership a ci-onto:KPIIndicator ;
	rdfs:label "Data Ownership"@en ;
	rdfs:comment "The Level of rights a client has over client data associated with a service."@en ;
	skos:definition "The Level of rights a client has over client data associated with a service."@en ;
	ci-onto:defined-in <http://csmic.org/wp-content/uploads/2011/09/SMI-Overview-110913_v1F.pdf>;
	ci-onto:defined-by ci-org:CSMIC;
	ci-onto:weight "1.0"^^xsd:double ;
