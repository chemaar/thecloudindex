<http://purl.org/cloudindex/ontology/>
    a owl:Ontology;
    owl:versionInfo "0.1";
    rdfs:label "Core Cloud Index structure and computation process"@en;
    rdfs:comment "This is the first version of the The CloudIndex taxonomy for representing QoS indicators and modeling an index."@en;
    dcterms:created "2012-05-20"^^xsd:date;
    dcterms:modified "2012-06-20"^^xsd:date;
    dcterms:title "The CloudIndex structure"@en;
    dcterms:author ci-people:Chema, ci-org:SEERC ;
    dcterms:publisher  ci-people:Chema;
    dcterms:contributor  ci-org:SEERC, ci-project:RELATE ;
    dcterms:license <http://opendatacommons.org/licenses/by/1.0/> .


#################################################################
#
#    Specific classes
#
#################################################################


ci-onto:KPIIndicator a owl:Class, rdfs:Class;
	rdfs:subClassOf rdfindex:Indicator;
	rdfs:label "KPI Indicator"@en ;
	rdfs:comment "Key Performance Indicator."@en;
.


#################################################################
#
#    Properties
#
#################################################################

### Metadata of observations: indicator|component, value, when, who

ci-ont:ref-area a rdf:Property, qb:DimensionProperty; 
	rdfs:label "URI to an Area"@en; 
	rdfs:subPropertyOf sdmx-dimension:ref-area; 
	rdfs:range skos:Concept; 
	qb:concept sdmx-concept:ref-area . 

ci-ont:ref-provider a rdf:Property, qb:DimensionProperty; 
	rdfs:label "URI to a provider"@en; 
	rdfs:subPropertyOf sdmx-dimension:ref-area; 
	rdfs:range skos:Concept; 
	qb:concept sdmx-concept:ref-area . 

ci-ont:ref-year  a rdf:Property, qb:DimensionProperty;
	rdfs:label "Period and year"@en;
	rdfs:subPropertyOf sdmx-dimension:ref-period;
	rdfs:range interval:Interval;
	qb:concept sdmx-concept:ref-period . 


ci-ont:ref-period  a rdf:Property, qb:DimensionProperty;
	rdfs:label "Period"@en;
	rdfs:subPropertyOf sdmx-dimension:ref-period;
	rdfs:range interval:Interval;
	qb:concept sdmx-concept:ref-period . 

ci-onto:value a owl:ObjectProperty, qb:MeasureProperty;
	rdfs:label "Observation's value"@en; 
	rdfs:comment "Values should be accessed through a proxy class"@en;
	rdfs:subPropertyOf sdmx-measure:obsValue; 
.

ci-onto:timestamp a owl:ObjectProperty, rdf:Property ;
	rdfs:label "Timestamp of an observation"@en ;
	rdfs:comment "It is used to specify the precise moment in which the observation has been generated."@en ;
.


### Metadata of definitions providers


ci-onto:defined-by  a owl:ObjectProperty, rdf:Property;
	rdfs:subPropertyOf  dcterms:source;
	rdfs:label "provider-link"@en;
	rdfs:comment "URI to the provider of a definition"@en;
. 


### Components


ci-component:ds
      a       void:Dataset ;
      rdfs:label "SEERC| SEQoS | Components"@en ;
      rdfs:comment "Dataset of components that aggregates different indicators and are part of a subindex."@en ;
      dcterms:created "2012-20-05"^^xsd:date;
      dcterms:modified "2012-08-31"^^xsd:date;
      dcterms:title "Core SeQoS ontology"@en;
      dcterms:author ci-people:Chema, ci-org:SEERC ;
