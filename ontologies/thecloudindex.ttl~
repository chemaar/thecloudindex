ci-indicator:Auditability  a ci-onto:KPIIndicator ;
	rdfs:label "Auditability"@en ;
	rdfs:comment "The ability of a client to verify that the service provider is adhering to the standards, processes, and policies that they follow."@en ;
	skos:definition "The ability of a client to verify that the service provider is adhering to the standards, processes, and policies that they follow."@en ;
	ci-onto:defined-in <http://csmic.org/wp-content/uploads/2011/09/SMI-Overview-110913_v1F.pdf>;
	ci-onto:defined-by ci-org:CSMIC;ci-onto:weight "1.0"^^xsd:double ;
	ci-onto:high-low ci-onto:High ;
	skos:notation "Auditability" ;
	time:intervalStarts "2013"^^xsd:int ;
	time:intervalFinishes "2013"^^xsd:int ;
	skos:broader ci-component:Accountability .


