      void:exampleResource ci-component:CommunicationsInfrastructure ;
      void:uriRegexPattern
              "http://purl.org/seerc/seqos/component/.+" ;
      void:vocabulary  rdfs: ,  skos: ;
      foaf:homepage <http://purl.org/seerc/seqos/> .




ci-component:Accountability a ci-onto:Component;
	rdfs:label "Accountability"@en;
	rdfs:comment "This component is used to measure the properties related to the service provider
organization. These properties may be independent of the service being provided."@en;
	ci-onto:defined-in <http://csmic.org/wp-content/uploads/2011/09/SMI-Overview-110913_v1F.pdf>;
	ci-onto:defined-by ci-org:CSMIC;
	#skos:broader ci-index:Readiness;
	rdfindex:weight "1"^^xsd:double;
	rdfindex:aggregates [ 		
		rdfindex:aggregation-operator rdfindex:OWA;
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:Auditability; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:Compliance; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:ContractingExperience; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:DataOwnership; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:EaseDoingBusiness; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:Governance; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:Ownership; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:ProviderStability; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:ProviderCertifications; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:ProviderSLAVerification; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:ProviderEthics; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:ProviderPersonnelRequirements; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:ProviderSupplyChain; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:ProviderSecurityCapabilities; 
			rdfindex:weight 0.06];
		rdfindex:part-of [
			rdfindex:dataset ci-indicator:Sustainability; 
			rdfindex:weight 0.06];
	];
	qb:structure 	ci:AccountabilityDSD ;
	.

	ci-component:Accountability skos:narrower ci-indicator:Auditability.
	ci-component:Accountability skos:narrower ci-indicator:Compliance.
	ci-component:Accountability skos:narrower ci-indicator:ContractingExperience.
	ci-component:Accountability skos:narrower ci-indicator:DataOwnership.
	ci-component:Accountability skos:narrower ci-indicator:EaseDoingBusiness.
	ci-component:Accountability skos:narrower ci-indicator:Governance.
	ci-component:Accountability skos:narrower ci-indicator:Ownership .
	ci-component:Accountability skos:narrower ci-indicator:ProviderStability .
	ci-component:Accountability skos:narrower ci-indicator:ProviderCertifications .
	ci-component:Accountability skos:narrower ci-indicator:ProviderSLAVerification .
	ci-component:Accountability skos:narrower ci-indicator:ProviderEthics .
	ci-component:Accountability skos:narrower ci-indicator:ProviderPersonnelRequirements .
